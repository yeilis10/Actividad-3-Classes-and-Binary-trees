class cat:
    def _init_(self,size):
        self.size=size
        self.color="orange"
        self.race="criollo"
        self.price= 50000
        self.name="malvado"
Rufo=cat("big")
print(Rufo.size)

class cat:
    def _init_(self,size,color,race,price,name):
        self.size=size
        self.color= color
        self.race= race
        self.price= price
        self.name= name


Rufo= cat("big","grey","bengali",60000,"dulce")
print(Rufo.size)
print(Rufo.color)
print(Rufo.race)
print(Rufo.price)
print(Rufo.name)


Rufo= cat("smal","brown","rogdoll",40000,"mangiro")
print(Rufo.size)
print(Rufo.color)
print(Rufo.race)
print(Rufo.price)
print(Rufo.name) 


Rufo= cat("smal","white","persa",60000,"maily")
print(Rufo.size)
print(Rufo.color)
print(Rufo.race)
print(Rufo.price)
print(Rufo.name) 

Rufo= cat("smal","black","himalayo",60000,"nube")
print(Rufo.size)
print(Rufo.color)
print(Rufo.race)
print(Rufo.price)
print(Rufo.name)
