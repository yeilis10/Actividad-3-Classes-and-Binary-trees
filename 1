lass birds:
    def _init_(self,size):
        self.size=size
        self.color="green"
        self.race="colibri chillon"
        self.price= 400000
        self.name="leo"
        
     
     
Rufo=birds("big")
print(Rufo.size)

class birds:
    def _init_(self,size,color,race,price,name):
        self.size=size
        self.color= color
        self.race= race
        self.price= price
        self.name= name
      
print(Rufo.size)
print(Rufo.size)
print(Rufo.size)
print(Rufo.size)
print(Rufo.size)
print(Rufo.name)



Rufo= birds("big","red,blue-yellou-green","guacamaya",800000,"roberto")
print(Rufo.size)
print(Rufo.color)
print(Rufo.race)
print(Rufo.price)
print(Rufo.name)

